{
    "name": "php-ctags",
    "displayName": "php ctags",
    "description": "",
    "version": "0.2.6",
    "publisher": "hcyang",
    "engines": {
        "vscode": "0.10.x"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onCommand:extension.ctag_generate",
        "onCommand:extension.ctag_search",
        "onLanguage:php"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.ctag_search",
                "title": "CTAGS:Search"
            },
            {
                "command": "extension.ctag_generate",
                "title": "CTAGS:Generate"
            },
            {
                "command": "extension.ctag_outline",
                "title": "CTAGS:Outline"
            }
        ],
        "keybindings": [
            {
                "command": "extension.ctag_search",
                "key": "ctrl+alt+p",
                "mac": "cmd+alt+p",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.ctag_outline",
                "key": "ctrl+alt+o",
                "mac": "cmd+alt+o",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.7.5",
        "vscode": "^0.11.0",
        "event-stream": "^3.3.2",
        "vscode-languageclient": "^2.2.1"
    },
    "__metadata": {
        "id": "533ea573-7e71-4f8b-9f2f-fd035e8cd32h",
        "publisherId": "f5654cd1-0372-4edb-92da-7b5f722b9295",
        "publisherDisplayName": "Heecheol,Yang"
    }
}